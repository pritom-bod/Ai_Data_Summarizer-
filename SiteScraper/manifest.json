{
  "manifest_version": 3,
  "name": "Full Website Scraper",
  "version": "1.0",
  "permissions": ["scripting", "tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html"
    
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
